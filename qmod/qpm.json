{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Quest Editor",
    "id": "qec",
    "version": "0.1.0",
    "url": null,
    "additionalData": {
      "overrideSoName": "libquesteditor.so"
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1",
        "pwsh ./scripts/restart-game.ps1"
      ],
      "log": [
        "pwsh ./scripts/start-logging.ps1 $0:?"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1 $0"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 -analyze"
      ]
    }
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.1.2",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3500.0.0",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^3.6.1",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.0",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.3",
      "additionalData": {}
    },
    {
      "id": "sombrero",
      "versionRange": "^0.1.40",
      "additionalData": {}
    }
  ]
}
